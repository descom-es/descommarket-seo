{
    "name": "descom/descommarket-seo",
    "description": "Descommarket SEO",
    "license": "MIT",
    "authors": [
        {
            "name": "Descom Support",
            "email": "soporte@descom.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^9.0|^10.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5|^11.5"
    },
    "autoload": {
        "psr-4": {
            "DescomMarket\\Seo\\": "src",
            "DescomMarket\\Seo\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DescomMarket\\Seo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DescomMarket\\Seo\\DescommarketSeoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
